{
  "name": "ehsan9/php-racing-game",
  "type": "project",
  "description": "CLI-based two-player vehicle racing game in PHP",
  "keywords": ["php", "cli", "challenge"],
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "symfony/console": "^7.0",
    "symfony/polyfill-php80": "^1.22"
  },
  "autoload": {
    "psr-4": {
      "RacingGame\\": "src/RacingGame/",
      "Foundation\\": "src/Foundation/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
